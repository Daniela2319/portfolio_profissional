{
  "version": 2,
  "builds": [
    {
      "src": "myprojeto/wsgi.py",
      "use": "@vercel/python",
      "config": { 
        "maxLambdaSize": "20mb", 
        "runtime": "python3.12" 
      }
    }
  ],
  "routes": [
    {
      "src": "static/(.*)",
      "dest": "staticfiles/$1"  
    },
    {
      "src": "/(.*)",
      "dest": "myprojeto/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "myprojeto.settings"
  }
}