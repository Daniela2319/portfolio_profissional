{
  "version": 2,
  "builds": [
    {
      "src": "myprojeto/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb","runtime": "python3.10" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "myprojeto/wsgi.py"
    }
  ],
  "build": {
    "env": {
      "DJANGO_SETTINGS_MODULE": "myprojeto.settings"
    },
    "commands": {
      "build": "npm install && npm run build"
    }
  }
}